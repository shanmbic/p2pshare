#!/usr/bin/env python

from p2pshare import Peer
from Tkinter import Tk
import sys

if __name__ == "__main__":
    root = Tk()
    root.geometry("640x320+300+300")
    ip=sys.argv[1]
    port=int(sys.argv[2]) 
    peer=Peer(ip=ip, port=port, parent=root)
    root.mainloop() 
    #peer.connect(ip='192.168.65.210', port=5214)"""